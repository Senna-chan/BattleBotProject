<Window x:Class="BattleBotClientWPF.Views.ColorPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BattleBotClientWPF.Views"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="ColorPicker" Height="150" Width="300">
    <Grid>
        <xctk:ColorPicker Margin="25,21,27,78" Name="Colorpicker"></xctk:ColorPicker>
        <Button Margin="139,89,85,10" Click="ButtonCancel_Click"  Content="Cancel"/>
        <Button Margin="216,89,8,10" Content="Ok" Click="ButtonOK_Click" />
    </Grid>
</Window>
